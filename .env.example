# Основные настройки
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/postgres?schema=public
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret"

# Провайдеры аутентификации
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# Настройки хранилища
# Выберите провайдера хранилища: "aws" или "yandex"
STORAGE_PROVIDER=yandex

# AWS S3 настройки
AWS_REGION="eu-central-1"
AWS_ACCESS_KEY_ID="your-aws-access-key-id"
AWS_SECRET_ACCESS_KEY="your-aws-secret-access-key"
AWS_S3_BUCKET_NAME="homio-uploads"
AWS_S3_BUCKET_URL="https://homio-uploads.s3.amazonaws.com"

# Yandex Object Storage настройки
YANDEX_STORAGE_ENDPOINT="https://storage.yandexcloud.net"
YANDEX_ACCESS_KEY_ID="your-yandex-access-key-id"
YANDEX_SECRET_ACCESS_KEY="your-yandex-secret-access-key"
YANDEX_BUCKET_NAME="your-bucket-name"
YANDEX_STORAGE_URL="https://storage.yandexcloud.net/your-bucket-name"

# Настройки приложения
NODE_ENV=development
APP_URL="http://localhost:3000"
NEXT_PUBLIC_API_URL="http://localhost:3000/api"

# Настройки почты
EMAIL_SERVER="smtp://user:pass@smtp.example.com:587"
EMAIL_FROM="noreply@example.com"

# Настройки Redis
REDIS_URL="redis://localhost:6379"

# Настройки логирования
LOG_LEVEL="info"

# Email (SMTP)
EMAIL_SERVER="smtp://username:password@smtp.example.com:587"
EMAIL_FROM="noreply@example.com"

# AWS S3
AWS_ACCESS_KEY_ID="your-aws-access-key-id"
AWS_SECRET_ACCESS_KEY="your-aws-secret-access-key"
AWS_REGION="eu-central-1"
AWS_BUCKET_NAME="your-bucket-name"

# Stripe
STRIPE_SECRET_KEY="your-stripe-secret-key"
STRIPE_WEBHOOK_SECRET="your-stripe-webhook-secret"

# Units Import API Token (for automated imports)
UNITS_IMPORT_API_TOKEN="your-secure-api-token"

# API Configuration
NEXT_PUBLIC_API_URL="http://localhost:3000/api"

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_AUTH=true

NEXT_PUBLIC_TEST_DEVELOPER_ID="your-developer-id"

# Next.js
NODE_ENV=development
OPENAI_API_KEY="your-openai-api-key"

_NEXT_INTL_TRAILING_SLASH=1

# Google Maps Configuration
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your-google-maps-api-key"

# OpenAI Configuration
OPENAI_API_KEY="your-openai-api-key"

# DeepSeek Configuration    
DEEPSEEK_API_KEY="your-deepseek-api-key"
DEEPSEEK_API_URL="https://api.deepseek.com"

# Telegram Configuration
TELEGRAM_BOT_TOKEN="your-telegram-bot-token"

# Dify Configuration
DIFY_API_KEY_DATASET="your-dify-dataset-api-key"
DIFY_API_KEY="your-dify-api-key"
DIFY_API_URL="http://localhost/v1"

# Heygen Configuration
NEXT_PUBLIC_HEYGEN_API_KEY="your-heygen-api-key"

# Node Options
NODE_OPTIONS='--max-old-space-size=8192' 