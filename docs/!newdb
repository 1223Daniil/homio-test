psql postgresql://postgres:postgres@localhost:5432/homio -c "CREATE TABLE IF NOT EXISTS \"Currency\" (id TEXT PRIMARY KEY DEFAULT gen_random_uuid(), code TEXT UNIQUE NOT NULL, symbol TEXT NOT NULL, name TEXT NOT NULL, rate DOUBLE PRECISION NOT NULL, \"isBaseCurrency\" BOOLEAN NOT NULL DEFAULT false, \"createdAt\" TIMESTAMP(3) NOT NULL DEFAULT CURRENT_TIMESTAMP, \"updatedAt\" TIMESTAMP(3) NOT NULL DEFAULT CURRENT_TIMESTAMP);"